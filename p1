#div3_list=[0 for i in range(1000)] 
#div5_list=[0 for i in range(1000)]
sum = 0
for i in range(1000) :
    if (i % 3) == 0 :
        #div3_list[i]=i
        sum = sum +i
    elif (i%5)==0 :
        #div5_list[i]=i
        sum = sum +i
